extends layout

block content
  for column in columns
    div.row
      div.col.s12
        table
          thead
            tr
              th.col.s1.
                Date
              th.col.s5.
                Headline
              th.col.s5.
                Keywords
              th.col.s1.
                Logged
          tbody
            for column in columns
              tr
                td.col.s1= moment(column.date).format('YYYY.MM.DD')
                td.col.s6= column.headline
                td.col.s4= column.keywords 
                td.col.s1= moment(column.logged).format('YYYY.MM.DD')
